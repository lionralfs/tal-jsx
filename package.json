{
  "name": "tal-jsx",
  "version": "1.0.1",
  "description": "A utility library which lets you use JSX Syntax in TAL (https://github.com/bbc/tal)",
  "main": "dist/taljsx.cjs",
  "umd:main": "dist/taljsx.umd.js",
  "module": "dist/taljsx.modern.js",
  "type": "module",
  "exports": {
    "require": "./dist/taljsx.cjs",
    "default": "./dist/taljsx.js"
  },
  "scripts": {
    "build": "rm -rf dist/ && microbundle src/index.js --name taljsx",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest"
  },
  "keywords": [
    "tal",
    "jsx",
    "tv-applications"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lionralfs/tal-jsx.git"
  },
  "author": "Lion Ralfs <lion.ralfs@gmail.com> (https://lionralfs.dev/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lionralfs/tal-jsx/issues"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/preset-env": "^7.4.5",
    "jest": "^27.4.7",
    "microbundle": "^0.14.2"
  }
}
